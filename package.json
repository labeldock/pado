{
  "name": "pado",
  "version": "0.4.1",
  "description": "Yet another functional programming helper",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sepalang/pado"
  },
  "scripts": {
    "start": "babel-node .src/test/src-test.js",
    "clear:dist": "rimraf ./*.js ./*.js.map ./functions ./modules",
    "build:default": "padoc .src/src ./ -m umd -s",
    "build:pack": "padoc --pack .src/src/pado.js ./pado.js -m umd -n pado -s",
    "build": "npm run clear:dist && npm run build:default && npm run build:pack",
    "test": "padoc --test .src/test",
    "labp": "padoc --pack .src/sample/labs/entry.js .src/sample/labs/pack.sample.js",
    "labe": "padoc --exec .src/sample/labs/entry.js -i",
    "labn": "node .src/sample/labs/pack.sample.js --interactive",
    "t": "padoc --exec .src/sample/use/operate.use.js"
  },
  "author": "AHN HO JUNG",
  "license": "MIT",
  "keywords": [
    "function",
    "functions"
  ],
  "devDependencies": {
    "@sepalang/padoc": "^0.7.16",
    "rimraf": "^2.6.2",
    "lodash": "^4.17.10"
  },
  "dependencies": {}
}
