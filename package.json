{
  "name": "useful-functions",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "1": "babel-node src/train.module.js",
    "start": "babel-node index.js",
    "build": "rimraf dist && babel src --out-dir dist",
    "test": "jest ''"
  },
  "keywords": [
    "function",
    "functions"
  ],
  "author": "AHN HO JUNG",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          },
          "loose": true,
          "useBuiltIns": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      "transform-es2015-modules-umd"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.2",
    "rimraf": "^2.6.2"
  }
}
