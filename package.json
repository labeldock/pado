{
  "name": "pado",
  "version": "0.1.1",
  "description": "Yet another functional programming helper",
  "main": "dist/pado.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/padowave/pado"
  },
  "scripts": {
    "start": "babel-node test/src-test.js",
    "clear:dist": "rimraf dist && mkdirp dist",
    "build": "npm run clear:dist && npm run babel:build:default && npm run babel:build:umd && npm run rollup:build && rimraf dist/commonjs",
    "babel:build:default": "babel src --out-dir dist/commonjs --plugins=transform-es2015-modules-commonjs",
    "babel:build:umd": "babel src --out-dir dist/umd --plugins=transform-es2015-modules-umd",
    "rollup:build": "rollup -c build.rollup.js",
    "s:web": "cd sample/web && npm start",
    "test": "jest ''",
    "cc": "padoc src -s compile -o dist/commonjs -t es6 -m commonjs",
    "cr": "padoc dist/commonjs/pado.js -s rollup -o dist/pado.js -m iife -n pd"
  },
  "author": "AHN HO JUNG",
  "license": "MIT",
  "keywords": [
    "function",
    "functions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "padoc": "padowave/padoc",
    "babel-jest": "^22.4.1",
    "jest": "^22.4.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}
