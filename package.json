{
  "name": "@sepalang/pado",
  "version": "0.2.1",
  "description": "Yet another functional programming helper",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sepalang/pado"
  },
  "scripts": {
    "start": "babel-node .util/test/src-test.js",
    "clear:dist": "rimraf ./*.js ./*.js.map ./functions ./modules ./web",
    "build:default": "padoc .src ./ -m umd -s",
    "build:pack:node": "padoc --pack .src/pado.js ./pado.js -m umd -n pado -s",
    "build:pack:web": "padoc --pack .src/pado.web.js ./web/pado.web.js -m umd -n pado -s",
    "build:pack": "npm run build:pack:node && npm run build:pack:web",
    "build": "npm run clear:dist && npm run build:default && npm run build:pack",
    "report:index": "madge .src/index.js --image .util/report/index.report.png",
    "report:pado": "madge .src/pado.js --image .util/report/pado.report.png",
    "report:modules": "madge .src/modules/index.js --image .util/report/modules.report.png",
    "report:functions": "madge .src/functions/index.js --image .util/report/functions.report.png",
    "report": "npm run report:index & npm run report:pado & npm run report:modules & npm run report:functions",
    "test:functions": "padoc --test .src/functions --verbose",
    "test:modules": "padoc --test .src/modules --verbose",
    "test:web": "padoc --test .src/web --verbose",
    "test": "padoc --test .src --verbose",
    "pack:a": "padoc --pack '.util/fnpack/case-a/*.js' .util/fnpack/case-a/packed -m es",
    "pack:b": "padoc --pack '.util/fnpack/case-b/*.js' .util/fnpack/case-b/packed -m es",
    "repl": "padoc --exec .util/repl/index.js -i -g"
  },
  "author": "AHN HO JUNG",
  "license": "MIT",
  "keywords": [
    "function",
    "functions"
  ],
  "devDependencies": {
    "@sepalang/padoc": "^0.8.16",
    "jquery": "^3.3.1",
    "jsdom": "^11.11.0",
    "madge": "^3.1.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
